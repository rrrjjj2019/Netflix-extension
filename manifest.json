{
  "manifest_version": 2,
  "name": "海豹軟綿神器",
  "description": "讓海豹可以好好軟綿的好東西",
  "version": "1.0.0",
  "icons": {
    "16": "icon2.PNG",
    "48": "icon2.PNG",
    "128": "icon2.PNG"
  },
  "browser_action": {
    "default_icon": "icon2.PNG",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/*"
      ],
      "js": [
        "execute.js"
      ]
    }
  ]
}